   "source": [
    "T = int(input())\n",
    "\n",
    "for t in range(1, T+1):\n",
    "    ans = 0\n",
    "\n",
    "    N = int(input())\n",
    "\n",
    "    sum_num = 0\n",
    "    for i in range(1, N+1):\n",
    "        if i % 2 == 0:\n",
    "            sum_num -= i\n",
    "        else:\n",
    "            sum_num += i\n",
    "    ans = sum_num\n",
    "    \n",
    "    print('#' + str(t) +' ' + str(ans))"
   ]
  }
