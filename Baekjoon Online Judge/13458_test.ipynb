   "source": [
    "N = int(input())\n",
    "a = list(map(int, input().split()))\n",
    "b, c = map(int, input().split())\n",
    "ans = 0\n",
    "\n",
    "for i in range(N):\n",
    "    if a[i] >= b:\n",
    "        d = a[i] - b\n",
    "        ans += d//c if d%c == 0 else d//c+1\n",
    "\n",
    "print(ans + N)"
   ]
  }
